 <input type="checkbox" id="hmenu" class="d-none position-absolute" >
    <label for="hmenu" id="lmenu" class="position-absolute start-0 top-0" style="width: 50px;"><img style="width: 50px;" src="https://cdn.icon-icons.com/icons2/1875/PNG/512/hamburgermenu_120234.png" alt="Menu" /></label>
    <aside>
        <a href='@Url.Action("HighScores")'>HighScores</a>
        <a href='@Url.Action("Index")'>Home</a>
    </aside>
<h1 class="text-white">High Scores</h1>
<br />
<table class="table w-50 table-hover">
    <thead class="thead-dark">
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Puntaje</th>
            <th>Fecha</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Puntajes p in ViewBag.puntajes)
        {
            <tr>
                <td class="text-primary">@p.Id</td>
                <td class="text-primary">@p.Username</td>
                <td class="text-primary">@p.Puntaje</td>
                <td class="text-primary">@p.Fecha</td>
            </tr>
        }
    </tbody>
</table>